<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<!-- Dependency Injection using Setter -->
	
	<bean id="batch" class="org.jsp.SpringAutowiring.Batch">
		<property name="names">
			<!-- Injecting List -->
			<list>
				<value>ABC</value>
				<value>PQR</value>
				<value>XYZ</value>
			</list>
		</property>
		<property name="phone_ns">
			<!-- Injecting Set -->
			<set>
				<value>999</value>
				<value>888</value>
				<value>777</value>
				<value>999</value>
			</set>
		</property>

		<property name="details">
			<map>
				<entry key="999" value="ABC"></entry>
				<entry key="888" value="PQR"></entry>
				<entry key="777" value="XYZ"></entry>
				<entry key="666" value="XYZ"></entry>
			</map>
		</property>

	</bean>

	<!-- Dependency Injection Using Constructor -->
	
	<bean id="batch1" class="org.jsp.SpringAutowiring.Batch">
		<constructor-arg name="names">
			<list>
				<value>venky</value>
				<value>vamsi</value>
				<value>azis</value>
			</list>
		</constructor-arg>
		<constructor-arg name="phone_ns">
		<set>
		<value>7097</value>
		<value>8998</value>
		<value>7676</value>
		</set>
		</constructor-arg>
		<constructor-arg name="details">
		<map>
		<entry key="7766" value="venkatesh"/>
		<entry key="6655" value="azis"/>
		<entry key="8800" value="vamsi"/>
		</map>
		</constructor-arg>
	</bean>
</beans>